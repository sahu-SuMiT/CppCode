#include <iostream>
using namespace std;
 
int main() {
 
    //for loop example
    cout << "For loop" << endl;
    for(int i = 0; i < 5; i++){
        cout << i << endl;
    }
    //while loop example
    cout << "While loop " << endl;
    int j = 0; 
    while(j < 5){
        cout << j << endl;
        j++;
    }
    return 0;
}
